{
  "name": "dash-validator",
  "version": "0.4.3",
  "description": "JS library to validate MPEG DASH manifests and segments",
  "main": "index.js",
  "engines": {
    "node": "6.9.1"
  },
  "repository": "https://github.com/Eyevinn/dash-validator-js",
  "scripts": {
    "docs": "$(npm bin)/jsdoc -c jsdoc.conf.json",
    "test": "$(npm bin)/karma start karma.conf.js",
    "clean": "rm -rf dist/* docs/*",
    "prebuild": "npm run clean && npm run test && npm run docs",
    "build": "browserify index.js -o dist/dashvalidator.js -t [ babelify --presets es2015 ] && $(npm bin)/uglifyjs dist/dashvalidator.js -o dist/dashvalidator.min.js",
    "version": "npm run build && git add -A dist && git add -A docs",
    "postversion": "git push && git push --tags"
  },
  "author": "Jonas Birme <jonas.birme@eyevinn.se> (http://eyvinn.github.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-espower": "^2.3.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-istanbul": "^2.0.0",
    "jasmine": "^2.5.2",
    "jasmine-jquery": "^2.1.1",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-generic-preprocessor": "^1.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "minami": "^1.1.1",
    "mockdate": "^2.0.1",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "request": "^2.79.0",
    "xmldom": "^0.1.27"
  }
}
