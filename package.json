{
  "name": "dash-validator",
  "version": "0.1.0",
  "description": "JS library to validate MPEG DASH manifests",
  "main": "index.js",
  "engines": {
    "node": "6.9.1"
  },
  "repository": "https://github.com/Eyevinn/dash-validator-js",
  "scripts": {
    "test": "$(npm bin)/karma start karma.conf.js",
    "clean": "rm -f dist/*",
    "prebuild": "npm run clean && npm run test",
    "build": "browserify index.js -o dist/dashvalidator.js -t babelify --presets es2015"
  },
  "author": "Jonas Birme <jonas.birme@eyevinn.se> (http://eyvinn.github.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-espower": "^2.3.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "jasmine": "^2.5.2",
    "jasmine-jquery": "^2.1.1",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-generic-preprocessor": "^1.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "request": "^2.79.0",
    "xmldom": "^0.1.27"
  }
